.container{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.display{
  height: 20px;
  width: 20px;
  margin: 20px 0 0 20px;
}
.model{
  height: 150px;
  width: 350px;
  position:relative;
  border-radius: 10px;
  margin: 20px auto;
}
.model image{
  height: 100%;
  width: 100%;
}
.model text{
  position: absolute;
  top: 0px;
  left: 9px;
  margin-top: 20px;
  color: white;
}