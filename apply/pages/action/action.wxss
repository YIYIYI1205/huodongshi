.container{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
}
.header,.title{
  background: #405f80;
}
.header text{
  float: right;
  color: white;
}
.title{
  text-align: center;
}
.title text{
  color: white;
  font-size: 20px;
}
.logo{
  display: flex;
  flex-direction: row;
  height: 70px;
  width: 100%;
}
.logo image{
  width: 100px;
  height: 50px;
  opacity: 0.6;
  margin-top: 10px; 
  margin-left: 30px;
}
.logo-text{
  line-height: 70px;
}
.apply,.myApply{
  height: 30px;
  width: 80px;
  background: #405f80;
  margin-top: 20px;
  margin-left: 45px;
  text-align: center;
  border-radius: 5px;
}
.apply text,.myApply text{
  font-size: 15px;
  color: white;
}
.table{
  width: 100%;
  text-align: center;
}
.table-title{
background: #405f80;
display: flex;
flex-direction: row;
border: 1px solid #555;
}
.table-title view{
  color: white;
  width: 25%;
  border-right: 1px solid #555;
}
.table-content{
display: flex;
flex-direction: row;
border: 1px solid #555;
}
.table-content view{
  width: 25%;
  border-right: 1px solid #555;
}
.myApply{
  margin: 300px auto;
}