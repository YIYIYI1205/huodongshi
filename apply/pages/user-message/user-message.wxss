page{
  background: #ccc
}
.container{
  height: 100%;
  width: 100%;
  display: flex;
  flex-direction: column;
}
.display{
  height: 20px;
  width: 20px;
  margin: 20px 0 0 20px;
}
.head{
  display: flex;
  flex-direction: column;
  text-align: center;
}
.head image{
  margin: 0 auto;
  width: 100px;
  height: 100px;
  border-radius: 50%;
  margin-top: 30px;
}
.number{
  color: #555;
  margin-top: 20px;
}
.hr{
  width: 80%;
  height: 1px;
  background: #555;
  margin:20px auto;
}
.middle{
  display: flex;
  flex-direction: column;
}
.box{
 display: flex;
 flex-direction: column;
}
.onerow{
 display: flex;
 flex-direction: row;
 height: 30px;
 margin: 10px 0;
}
.onerow image{
  height: 30px;
  width: 30px;
  margin-left:40px; 
}
 .box text{
  margin-left: 100px;
  line-height: 30px;
} 